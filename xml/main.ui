<?xml version="1.0" encoding="UTF-8"?>
<interface>
    <object id="window" class="GtkWindow">
        <property name="visible">True</property>
        <property name="title">Rise of the Witch-King Unofficial Launcher</property>
        <property name="default-width">600</property>
        <property name="default-height">400</property>
        <property name="border-width">10</property>
        <child>
            <object id="grid" class="GtkGrid">
                <property name="visible">True</property>
                <child>
                    <object class="GtkHeaderBar" id="header">
                        <property name="visible">True</property>
                        <child type="title">
                            <object class="GtkStackSwitcher" id="tabs">
                                <property name="visible">True</property>
                                <property name="stack">stack</property>
                            </object>
                        </child>
                    </object>
                </child>
                <child>
                    <object class="GtkStack" id="stack">
                        <property name="visible">True</property>
                    </object>
                </child>
                <child>
                    <object id="rotwk_run" class="GtkButton">
                        <property name="visible">True</property>
                        <property name="label">Play RotWK</property>
                    </object>
                    <packing>
                        <property name="left-attach">0</property>
                        <property name="top-attach">0</property>
                    </packing>
                </child>
                <child>
                    <object id="edain_run" class="GtkButton">
                        <property name="visible">True</property>
                        <property name="label">Play Edain</property>
                    </object>
                    <packing>
                        <property name="left-attach">0</property>
                        <property name="top-attach">1</property>
                    </packing>
                </child>
                <child>
                    <object id="botta_run" class="GtkButton">
                        <property name="visible">True</property>
                        <property name="label">Play BotTA</property>
                    </object>
                    <packing>
                        <property name="left-attach">0</property>
                        <property name="top-attach">2</property>
                    </packing>
                </child>
                <child>
                    <object id="rotwk_update" class="GtkButton">
                        <property name="visible">True</property>
                        <property name="label">Update RotWK</property>
                    </object>
                    <packing>
                        <property name="left-attach">5</property>
                        <property name="top-attach">0</property>
                    </packing>
                </child>
                <child>
                    <object id="edain_update" class="GtkButton">
                        <property name="visible">True</property>
                        <property name="label">Update Edain</property>
                    </object>
                    <packing>
                        <property name="left-attach">5</property>
                        <property name="top-attach">1</property>
                    </packing>
                </child>
                <child>
                    <object id="botta_update" class="GtkButton">
                        <property name="visible">True</property>
                        <property name="label">Update BotTA</property>
                    </object>
                    <packing>
                        <property name="left-attach">5</property>
                        <property name="top-attach">2</property>
                    </packing>
                </child>
                <child>
                    <object id="quit" class="GtkButton">
                        <property name="visible">True</property>
                        <property name="label">Quit</property>
                    </object>
                    <packing>
                        <property name="left-attach">0</property>
                        <property name="top-attach">4</property>
                        <property name="width">6</property>
                    </packing>
                </child>
            </object>
            <packing>
            </packing>
        </child>
    </object>
</interface>
